  "devDependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "expect": "^1.20.2",
    "flow-bin": "^0.32.0",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "react-scripts": "0.4.1",
    "redux-devtools": "^3.3.1",
    "redux-mock-store": "^1.1.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "npm run test:lint-js && npm run test:type-checks && npm run test:unit",
    "test:browser": "react-scripts test --env=jsdom",
    "test:lint-js": "eslint ./src ./test",
    "test:type-checks": "flow",
    "test:unit": "NODE_ENV=test mocha --recursive --compilers js:babel-register",
    "test:watch": "npm test -- --watch"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js",
    "rules": {
      "semi": 1
    }
  }
